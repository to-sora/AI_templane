{
    version: "v34",
    data_dir: "./data4/",
    cache_dir: "./data/cache/",
    logs_dir: "./logs/",
    batch_size: 10,
    num_workers: 2,
    lr: 0.0001,
    scheduler_params: {
        mode: "min",
        factor: 0.5,
        patience: 5,
    },
    weight_decay: 0.0001,
    debug: false,
    epochs: 200,
    gradient_accumulation_steps: 30,
    model_save_path: "./models/model_data",
    num_classes: {
        note_type: 2,
        instrument: 5,
        pitch: 128,
    },
    model_structure: {
        dimension: 1023,
        num_encoder_layers: 2,
        num_decoder_layers: 2,
        backbone_type: "CustomTokenizedBackbone",
        pretrain: false,
        positional_embedding: "sinusoid",
        time_series_type: "default",
        number_of_layers: 7,
        regression_activation_last_layer: "relu",
        classification_activation_last_layer: "None",
        MLP_skip: 3,
        add_time_dimension: true,
    },
    max_objects: 150,
    cost_note_type: 1,
    cost_instrument: 1,
    cost_pitch: 10,
    cost_regression: 1,
    start_time_scaler: 1,
    duration_scaler: 1,
    velocity_scaler: 1,
    save_by_spoch: 1,
    output_normalize: true,
    transforms: {
        type: "CQT",
        params: {
            hop_length: 64,
            n_bins: 240,
            bins_per_octave: 48,
            fmin: 27.5,
        },
    },
    loss: {
        use_softmax: true,
    },
    device: "cuda",
    freeze_backbone: false,
    freeze_transformer: false,
    freeze_heads: false,
    freeze_all: false,
    no_prompt: false,
}